{
    "name": "Header Craft",
    "description": "Effortlessly customize HTTP headers for efficient testing and a tailored web experience",
    "version": "0.9",
    "manifest_version": 3,
    "icons": {},
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "declarativeNetRequest",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}